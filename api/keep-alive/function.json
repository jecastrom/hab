{
  "bindings": [
    {
      "name": "keepAliveTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"  // Every 5 minutes (adjust as needed, e.g., "0 */10 * * * *" for 10min)
    }
  ],
  "scriptFile": "index.js"  // Explicitly points to the script for clarity
}